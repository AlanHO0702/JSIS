@page "{PaperNum}"
@model AJNdJourSubModel
@{
    ViewData["TableTitle"] = $"傳票明細（{Model.MasterKey}）";
    ViewData["DictTableName"] = Model.TableName;
    ViewData["AddApiUrl"] = "/api/AJNdJourMain";        
    ViewData["SubRouteTemplate"] = "/AJN/AJNdJourSub/{PaperNum}";
    ViewData["QueryRedirectUrl"] = "/AJN/AJNdJourMain";    // 返回的主頁面
    ViewData["DeleteApiUrl"] = "AJNdJourMain";   
    ViewData["ReportSpName"] = "AJNdJourPaper_CIR";
    ViewData["ActionRailPartial"] = "~/Pages/AJN/JourSubCustomButton.cshtml"; // ← 指定本頁的按鈕 
}
<script>window._dictTableName = "@(ViewData["DictTableName"] ?? Model.TableName)";</script>
@await Html.PartialAsync("~/Pages/Shared/_TableDetailPartial.cshtml", Model)
@await Html.PartialAsync("_FieldDictModal", Model.FieldDictList, ViewData)
<script src="~/js/fieldDictModal.js"></script>


