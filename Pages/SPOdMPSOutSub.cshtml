    ViewData["TableTitle"] = $"銷貨單明細（{Model.MasterKey}）";
    ViewData["DictTableName"] = Model.TableName;
    ViewData["AddApiUrl"] = "/api/SPOdMPSOutMain";         // /api/SpodOrderSub
