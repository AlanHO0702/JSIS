@page
@model SpodOrdersModel
@{
    ViewData["TableTitle"] = "銷售訂單列表";
    ViewData["SubRouteTemplate"] = "/SpodOrderSub/{PaperNum}";
    // 這裡 PaginationVm 也直接丟給 Model
    ViewData["PaginationVm"] = new PaginationModel {
    PageNumber = Model.PageNumber,
    TotalPages = Model.TotalPages,
    RouteUrl = "/SPOdOrderMain"
    };
}
@await Html.PartialAsync("~/Pages/Shared/_TableListPartial.cshtml", Model)
@await Html.PartialAsync("_FieldDictModal", Model.FieldDictList, ViewData)
<script src="~/js/fieldDictModal.js"></script>
